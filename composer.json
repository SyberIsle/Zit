{
  "name": "syberisle/zit",
  "description": "Zit is a simple dependency injector based heavily on Pimple. It aims to provide the same simplicity as Pimple while offering a slightly more robust object interface.",
  "version": "3.0.0",
  "type": "library",
  "keywords": [
    "dependency injection",
    "dependency",
    "di",
    "container",
    "zit",
    "pimple"
  ],
  "homepage": "https://github.com/syberisle/Zit",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://cmorrell.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/syberisle/Zit/issues"
  },
  "autoload": {
    "psr-4": {
      "Zit\\": "lib/Zit"
    }
  },
  "require": {
    "php": ">=5.4.0",
    "container-interop/container-interop": "^1.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/syberisle/Zit"
    }
  ]
}
